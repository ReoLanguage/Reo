section treo.conn;

fifo<k>(a?, b!) { 
	
	if (k <= 0) { 
		a = b 
	} else {
		fifo[k](a,b)
	}
	
	fifo1(a?, b!) {
		#WA
		q0* -- {a}, true -> q1
		q1 -- {b}, true -> q0
	}
	
	for i = 0..n {
		if (i == 0) {
			fifo[0](a?,b!) {
				a = b
			}
		} else (i > 0) {
			fifo[i](a?,b!) {
				fifo1(a,x) 
				fifo[i-1](x,b)
			}
		}
	}
}